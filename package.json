{
  "name": "get-date-format",
  "version": "0.0.1",
  "description":
    "Module that tries to get the date format out of sample of dates",
  "main": "./dist/index.js",
  "directories": {
    "config": "./config",
    "dist": "./dist",
    "src": "./src"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=PROD webpack --progress --colors",
    "build:watch":
      "cross-env NODE_ENV=DEV webpack --progress --colors --watch --growl",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=TEST mocha-webpack",
    "test:unit:watch": "cross-env NODE_ENV=TEST mocha-webpack --watch --growl",
    "prestart": "npm run build",
    "prepublish": "npm run build",
    "start": "node ./bin/main"
  },
  "repository": {
    "type": "git",
    "url":
      "git+https://github.com/resisttheurge/babel-webpack-package-boilerplate.git"
  },
  "keywords": ["date", "get-date-format", "extract-date", "get-date"],
  "author": "Domagoj Katavic <domo1503@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url":
      "https://github.com/resisttheurge/babel-webpack-package-boilerplate/issues"
  },
  "homepage":
    "https://github.com/resisttheurge/babel-webpack-package-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-0": "^6.22.0",
    "bind-infix-proxy": "^1.0.0",
    "chai": "^4.1.2",
    "cross-env": "^3.1.4",
    "eslint": "^4.7.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jsverify": "^0.7.4",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "prettier": "^1.7.0",
    "source-map-support": "^0.4.11",
    "webpack": "^2.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": ["DEV", "PROD", "TEST"]
  },
  "dependencies": {
    "array-flatten": "^2.1.1",
    "js-combinatorics": "^0.5.2",
    "moment": "^2.18.1"
  }
}
